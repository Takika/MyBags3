			</OnShow>
			<OnHide>
				PlaySound(SOUNDKIT.IG_BACKPACK_CLOSE)
			</OnHide>
			<OnMouseDown>
				mybags = self.self
				if not mybags.GetOpt("Lock") then
					self:GetParent():StartMoving()
				end
			</OnMouseDown>
			<OnMouseUp>
				mybags = self.self
